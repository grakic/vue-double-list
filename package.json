{
  "name": "vue-double-list",
  "description": "Double List Selection Component for Vue.js",
  "version": "0.1.0",
  "author": {
    "name": "Goran Rakic",
    "email": "grakic@devbase.net"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "license": "MIT",
  "main": "dist/vue-double-list.common.js",
  "unpkg": "dist/vue-double-list.js",
  "module": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grakic/vue-double-list.git"
  },
  "bugs": {
    "url": "https://github.com/grakic/vue-double-list/issues"
  },
  "homepage": "https://github.com/grakic/vue-double-list#readme",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.12.1",
    "eslint-config-standard": "^10.2.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.1.0-beta.28",
    "webpack-merge": "^4.1.0"
  }
}
